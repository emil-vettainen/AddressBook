<UserControl x:Class="AddressBook.WPF.Views.ContactListView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AddressBook.WPF.Views"
             
             xmlns:viewmodels="clr-namespace:AddressBook.WPF.ViewModels"
             xmlns:models="clr-namespace:AddressBook.Shared.Models;assembly=AddressBook.Shared"
             
             d:DataContext="{d:DesignInstance Type=viewmodels:ContactsListViewModel}"
             
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        
        <StackPanel>
            <Label Content="Contacts list" />
            <Button Content="Navigate to Add" Command="{Binding NavigateToAddContactCommand}" />
        </StackPanel>
    </Grid>
</UserControl>
